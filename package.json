{
  "name": "tk-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "migration:generate": "ts-node node_modules/typeorm/cli.js migration:generate --config ./orm.config.json --name",
    "migration:run": "ts-node node_modules/typeorm/cli.js migration:run --config ./orm.config.json",
    "migration:revert": "ts-node node_modules/typeorm/cli.js migration:revert --config ./orm.config.json",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "watch": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^7.0.8",
    "@nestjs/core": "^7.0.8",
    "@nestjs/jwt": "^7.0.0",
    "@nestjs/passport": "^7.0.0",
    "@nestjs/platform-express": "^7.0.8",
    "@nestjs/typeorm": "^7.0.0",
    "@nestrx/twilio": "0.0.2",
    "@sendgrid/mail": "^7.0.1",
    "@types/bcrypt": "^3.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/jsonwebtoken": "^8.3.9",
    "@types/otplib": "^10.0.0",
    "@types/passport": "^1.0.3",
    "@types/sendgrid": "^4.3.0",
    "@types/sendmail": "^1.4.2",
    "@types/twilio": "^2.11.0",
    "bcrypt": "^4.0.1",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.14.0",
    "mysql": "^2.18.1",
    "node-localstorage": "^2.1.6",
    "npm-check-updates": "^4.1.2",
    "otplib": "^12.0.1",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "rand-token": "^1.0.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.5",
    "sendgrid": "^5.2.3",
    "sendmail": "^1.6.1",
    "ts-mapper": "^1.2.4",
    "twilio": "^3.42.2",
    "typeorm": "^0.2.24"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.1.2",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.8",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.6",
    "@types/jest": "25.2.1",
    "@types/node": "^13.13.2",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "jest": "^25.4.0",
    "prettier": "^2.0.5",
    "supertest": "^4.0.2",
    "ts-jest": "25.4.0",
    "ts-loader": "^7.0.1",
    "ts-node": "^8.9.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.8.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
